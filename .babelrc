{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"],
          "node": 10
        },
        "modules": "commonjs",
        "loose": true
      }
    ],
    "@babel/preset-flow"
  ]
}
